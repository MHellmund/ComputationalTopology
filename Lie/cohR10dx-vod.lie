### Lie group of SO(D) ###
sod(int d)=
{
loc gn=4-2*(d-d/2*2);
loc rankd=d/2;
loc sogroup=T0;
if d==6 then sogroup=A3 else
 if d>4 then sogroup=Lie_group(gn,rankd) else
   if d==4 then sogroup=A1A1 else
       if d==3 then sogroup=A1 else
            if d==2 then sogroup=T1 else
                  sogroup=T0;
            fi;
      fi;
   fi;
 fi;
fi;
sogroup
}
### the vector representation of SO(d) ###
vod(int d)=
{
loc rankd=d/2;
loc vd=X [];
if d==0 then vd=X [] else
   if d==1 then vd=X [0] else
      if d==2 then vd=X [2]+ X [-2] else
         if d==3 then vd=X [2] else
            if d==4 then vd=X[1,1] else
               if d==6 then vd=[0,1,0] else
                  if d>=2 then vd=X (1+null(rank-1))
                  fi;
               fi;
            fi;
         fi;
      fi;
   fi;
fi;
print(vd);
