##### calculate candidate cohomologies #####
coh(int k)=
{
loc t=poly_null(rank);
loc iMax=k/2;
if k>2*dimD then iMax=dimD; fi;
for i=0 to iMax do
        t=t+h(k-2*i, i);
        od;
        t;
}
